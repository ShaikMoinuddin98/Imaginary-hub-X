        <script>
            let startTime;
            let totalScrollDepth = 0;
            let articleId=location.pathname.split("/")[3]
            let hasViewed = false;
            let shared = 0;
         

            document.querySelector("#share-button").addEventListener("click",()=>{
                shared+=1
            })
            
            // Start tracking time when the page loads
            window.onload = function () {
                startTime = Date.now();
                hasViewed = true; // Consider a view when the page loads
            };

            // Track scroll depth
            window.addEventListener('scroll', () => {
                console.log(totalScrollDepth)
                const scrollTop = window.scrollY;
                const windowHeight = window.innerHeight;
                const docHeight = document.body.scrollHeight;
                const scrollDepth = (scrollTop + windowHeight) / docHeight * 100;

                if (scrollDepth > totalScrollDepth) {
                    totalScrollDepth = scrollDepth;
                }


            });


            window.onbeforeunload = async function () {
                const timeSpent = (Date.now() - startTime) / 1000; // Time spent in seconds

                // Prepare data to send
                const engagementData = {
                    articleId:articleId ,
                    views: hasViewed ? 1 : 0,
                    shares: shared,
                    
                    totalScrollDepth,
                   
                    timeSpent,
                };
                const blob = new Blob([JSON.stringify(engagementData)], { type: 'application/json' });

                // Send data to the server
                navigator.sendBeacon('/update-engagement',blob);
            };
        </script>

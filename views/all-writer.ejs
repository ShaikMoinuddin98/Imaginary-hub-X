                <div class="search-container">
                    <input type="text" placeholder="Search by authour name" id="searchInput">
                    <button onclick="performSearch()">Search</button>
                </div>

                <script>
                    function getBase64FromBlob(blob) {
                        return new Promise((resolve, reject) => {
                            const reader = new FileReader();
                            reader.onloadend = () => resolve(reader.result);
                            reader.onerror = reject;
                            reader.readAsDataURL(blob);
                        });
                    }
                    function performSearch() {
                        const query = document.getElementById('searchInput').value;
                        if (query) {
                            fetch("/search-writer", {
                                method: "POST",
                                headers: {
                                    "Content-Type": "application/json"
                                },
                                body: JSON.stringify({ name: query })
                            })
                                .then((res) => res.json())
                                .then(async (result) => {
                                    let container = document.querySelector(".authors-grid")
                                    let inhtml = ""
                                    for (item of result.data) {

                                        let src
                                        if (item.img.data.data) {
                                            const byteArray = new Uint8Array(item.img.data.data);
                                            const blob = new Blob([byteArray], { type: item.img.contentType });
                                            src = await getBase64FromBlob(blob);
                                        }
                                        else {
                                            src = '/assests/img/sample photo.jpg'
                                        }
                                        html = `   
                                        <div role="listitem" class="w-dyn-item"><a href='profile/${encodeURIComponent(item.username)}' class="author-item w-inline-block">
                                <div class="author-item-img-wrap">
                                    <img alt="" loading="lazy"
                                    src=${src}
                                        sizes="(max-width: 479px) 92vw, (max-width: 767px) 95vw, (max-width: 991px) 30vw, 31vw"
                                        class="img-cover-absolute" />
                                    <div class="img-btm-panel"></div>
                                    <div class="img-top-panel"></div>
                                    <div class="img-right-panel"></div>
                                    <div class="img-left-panel"></div>
                                </div>
                                <div class="author-item-content-grid">
                                    <div id="w-node-_418f7502-d7d2-111b-f841-9a52ca80df9a-d34fe257"
                                        class="author-item-name-wrap">
                                        <h4 class="title-xsmall">${item.username}</h4>
                                    </div>
                                    <p id="w-node-f537bedc-44c6-d782-9843-0a58397820de-d34fe257">${item.profession}
                                    </p>
                                </div>
                                <div class="line-trigger">
                                    <div class="line-moves"></div>
                                </div>
                            </a></div>`
                                        inhtml += html
                                    }
                                    if (inhtml != "") {
                                        container.innerHTML = inhtml
                                    }
                                    else {
                                        container.innerHTML = "<h2>No Result</h2>"
                                    }
                                })
                        } else {
                            alert("Please enter a search term.");
                        }
                    }
                </script>

                <style>
                    /* Center search bar */
